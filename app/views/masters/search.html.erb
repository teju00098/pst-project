
<h2>Search Results </h2>

<div class="media">
<div class="media-body">
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>CountName</th>
      <th>StoreCode</th>
      <th>StoreName</th>
      <th>VenderCode</th>
      <th>VenderName</th>
      <th>DeptCode</th>
      <th>DeptName</th>
      <th>SubDeptCode</th>
      <th>SubDeptName</th>
      <th>Class</th>
      <th>ClassName</th>
      <th>SubClass</th>
      <th>SubClassName</th>
      <th>SKUType</th>
      <th>SpecialAttribute</th>
      <th>SKU</th>
      <th>BarcodeIBC</th>
      <th>BarcodeSBC</th>
      <th>ProductName</th>
      <th>Brand</th>
      <th>Model</th>
      <th>UnitOfMeasure</th>
      <th>Stock</th>
      <th>Pack Size</th>
      <th>Cost</th>
      <th>Retail price</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @masters.each do |master| %>
      <tr>
        <td><%= master.CountName %></td>
        <td><%= master.StoreCode %></td>
        <td><%= master.StoreName %></td>
        <td><%= master.VenderCode %></td>
        <td><%= master.VenderName %></td>
        <td><%= master.DeptCode %></td>
        <td><%= master.DeptName %></td>
        <td><%= master.SubDeptCode %></td>
        <td><%= master.SubDeptName %></td>
        <td><%= master.Class %></td>
        <td><%= master.ClassName %></td>
        <td><%= master.SubClass %></td>
        <td><%= master.SubClassName %></td>
        <td><%= master.SKUType %></td>
        <td><%= master.SpecialAttribute %></td>
        <td><%= master.SKU %></td>
        <td><%= master.BarcodeIBC %></td>
        <td><%= master.BarcodeSBC %></td>
        <td><%= master.ProductName %></td>
        <td><%= master.Brand %></td>
        <td><%= master.Model %></td>
        <td><%= master.UnitOfMeasure %></td>
        <td><%= master.Stock %></td>
        <td><%= master.PackSize %></td>
        <td><%= master.Cost %></td>
        <td><%= master.RetailPrice %></td>
        <td><%= master.Status %></td>
        <td><%= link_to 'Show', master %></td>
        <td><%= link_to 'Edit', edit_master_path(master) %></td>
        <td><%= link_to 'Destroy', master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>

<p>
  <%= link_to 'Back to all master data', masters_path, class: 'btn btn-mini btn-warning' %>
</p>

<br>



